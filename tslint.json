{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    //"indent": [true, "tabs"],
    "semicolon": [true, "always"],
    "ordered-imports": false,
    "align": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "no-console": false,
    "object-literal-sort-keys": false,
    "eofline": false
  }
}
